<?xml version='1.0' encoding='UTF-8'?>
<MyPythonApp>
  <Mainconf>
    <DebugLogs>Fill me with an boolean</DebugLogs>
    <MetricsUrl>Fill me with an string</MetricsUrl>
    <ControlTimeout>Fill me with an integer</ControlTimeout>
    <ControlPort>Fill me with an integer</ControlPort>
  </Mainconf>
  <NamedSessions>
    <!--This is a dictionary-->
    <Item>
      <Key>Fill me with an string</Key>
      <Val>
        <Id>Fill me with an integer</Id>
        <AppName>Fill me with an string</AppName>
        <Conn>Insert connection string here proto:xxx.xxx.xxx.xxx:pppp</Conn>
      </Val>
    </Item>
  </NamedSessions>
  <ExtraSessions>
    <!--This is a List-->
    <SessionConfig>
      <Id>Fill me with an integer</Id>
      <AppName>Fill me with an string</AppName>
      <Conn>Insert connection string here proto:xxx.xxx.xxx.xxx:pppp</Conn>
    </SessionConfig>
  </ExtraSessions>
  <NameToUser>
    <!--This is a dictionary-->
    <Item>
      <Key>Fill me with an string</Key>
      <Val>
        <!--This is a union, the following variants are possible, only one can be present-->
        <Int>Fill me with an integer</Int>
        <UserConfig>
          <Name>Fill me with an string</Name>
          <AuthToken>Fill me with an string</AuthToken>
        </UserConfig>
      </Val>
    </Item>
  </NameToUser>
</MyPythonApp>
